<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@color/bgmenu"
    tools:context=".MainActivity2">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="438dp"
            android:layout_height="115dp"
            android:layout_marginTop="680dp"
            android:background="@color/endpanel_main"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView52"
            android:layout_width="0dp"
            android:layout_height="115dp"
            android:layout_marginBottom="498dp"
            android:background="@color/endpanel_main"
            android:visibility="visible"
            app:layout_constraintBottom_toTopOf="@+id/imageView5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.515"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="MissingConstraints" />

        <LinearLayout
            android:layout_width="413dp"
            android:layout_height="110dp"
            android:layout_marginTop="650dp"
            android:orientation="horizontal"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageButton
                android:id="@+id/ImageButtonHome"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:layout_margin="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginBottom="15dp"
                android:layout_weight="1"
                android:background="@color/endpanel_main"
                android:contentDescription="@string/app_name"
                android:scaleType="fitCenter"
                android:src="@drawable/home"
                tools:src="@drawable/home"
                tools:srcCompat="@drawable/home" />

            <ImageButton
                android:id="@+id/imageButtonBascet"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:layout_margin="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginBottom="15dp"
                android:layout_weight="1"
                android:background="@color/endpanel_main"
                android:contentDescription="@string/app_name"
                android:scaleType="fitCenter"
                android:src="@drawable/backet"
                tools:src="@drawable/backet"
                tools:srcCompat="@drawable/backet" />

            <ImageButton
                android:id="@+id/logoutButton"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:layout_margin="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginBottom="15dp"
                android:layout_weight="1"
                android:background="@color/endpanel_main"
                android:contentDescription="@string/app_name"
                android:scaleType="fitCenter"
                android:src="@drawable/imageenter"
                tools:src="@drawable/imageenter"
                tools:srcCompat="@drawable/imageenter" />
        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="428dp"
        android:layout_height="491dp"
        android:layout_marginTop="122dp"
        android:layout_marginBottom="110dp"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/button_latte"
                    android:layout_width="387dp"
                    android:layout_height="90dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    android:backgroundTint="@android:color/transparent"
                    android:gravity="bottom|center"
                    android:onClick="onLatteButtonClick"
                    android:paddingBottom="20dp"
                    android:text="@string/latte"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    app:cornerRadius="0dp" />

                <ImageView
                    android:layout_width="110dp"
                    android:layout_height="90dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:backgroundTint="@android:color/transparent"
                    android:src="@drawable/latte" />


            </FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/button_cappuccino"
                    android:layout_width="387dp"
                    android:layout_height="90dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    android:backgroundTint="@android:color/transparent"
                    android:gravity="bottom|center"
                    android:onClick="onCappuccinoButtonClick"
                    android:paddingBottom="20dp"
                    android:text="@string/cappuchino"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    app:cornerRadius="0dp" />

                <ImageView
                    android:layout_width="110dp"
                    android:layout_height="91dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:backgroundTint="@android:color/transparent"
                    android:src="@drawable/cappuchhino"
                    android:visibility="visible" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/button_raf"
                    android:layout_width="400dp"
                    android:layout_height="90dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    android:backgroundTint="@android:color/transparent"
                    android:gravity="bottom|center"
                    android:onClick="onRafButtonClick"
                    android:paddingBottom="20dp"
                    android:text="@string/raf"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    android:visibility="visible"
                    app:cornerRadius="0dp" />

                <ImageView
                    android:layout_width="110dp"
                    android:layout_height="91dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:backgroundTint="@android:color/transparent"
                    android:src="@drawable/raf"
                    android:visibility="visible" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/button_americano"
                    android:layout_width="396dp"
                    android:layout_height="90dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    android:backgroundTint="@android:color/transparent"
                    android:onClick="onAmericanoButtonClick"
                    android:text="@string/americano"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    app:cornerRadius="0dp" />

                <ImageView
                    android:layout_width="110dp"
                    android:layout_height="90dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:backgroundTint="@android:color/transparent"
                    android:src="@drawable/americano"
                    android:visibility="visible" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/button_flatwhite"
                    android:layout_width="387dp"
                    android:layout_height="90dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    android:backgroundTint="@android:color/transparent"
                    android:onClick="onFlatWhiteButtonClick"
                    android:text="@string/flat_white"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    app:cornerRadius="0dp" />

                <ImageView
                    android:layout_width="110dp"
                    android:layout_height="90dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:backgroundTint="@android:color/transparent"
                    android:src="@drawable/flatwhite"
                    android:visibility="visible" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/button_macchiato"
                    android:layout_width="387dp"
                    android:layout_height="90dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    android:backgroundTint="@android:color/transparent"
                    android:onClick="onMacchiatoButtonClick"
                    android:text="@string/macchiato"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    app:cornerRadius="0dp" />

                <ImageView
                    android:layout_width="110dp"
                    android:layout_height="90dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:backgroundTint="@android:color/transparent"
                    android:src="@drawable/macchiato"
                    android:visibility="visible" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/button_lungo"
                    android:layout_width="387dp"
                    android:layout_height="90dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    android:backgroundTint="@android:color/transparent"
                    android:onClick="onLungoButtonClick"
                    android:text="@string/lungo"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    app:cornerRadius="0dp" />

                <ImageView
                    android:layout_width="110dp"
                    android:layout_height="90dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:backgroundTint="@android:color/transparent"
                    android:src="@drawable/lungo"
                    android:visibility="visible" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <Button
                    android:id="@+id/button_brave"
                    android:layout_width="387dp"
                    android:layout_height="90dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    android:backgroundTint="@android:color/transparent"
                    android:onClick="onBraveButtonClick"
                    android:text="@string/brave"
                    android:textColor="#000000"
                    android:textSize="20sp"
                    app:cornerRadius="0dp" />

                <ImageView
                    android:layout_width="110dp"
                    android:layout_height="90dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:backgroundTint="@android:color/transparent"
                    android:background="@android:color/transparent"
                    android:src="@drawable/breve"
                    android:visibility="visible" />
            </FrameLayout>

        </LinearLayout>

    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>